<h2><%= @user_grading_set.grading_set.name %></h2>
<hr/>
<div id="react-app-container" 
  data-authenticity-token="<%= form_authenticity_token %>"
  data-user-grading-set="<%= JSON.dump(@user_grading_set.as_json(
    include: [
      :user_grading_set_images,
      grading_set: {
        include: [grading_set_images: {
          include: {
            images_for_grading: {methods: [
              :image_url_list,
              :image_url_main
            ]}
          }
        }]
      }
    ]
  )) %>"
  data-grading-type="<%= 'FGSGrading' %>"
></div>

<%= javascript_pack_tag 'grading' %>
